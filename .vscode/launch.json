{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/main.exe",
            "args": ["0","1"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
